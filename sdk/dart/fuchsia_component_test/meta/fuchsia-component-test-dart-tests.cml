{
    include: [
        "sys/component/realm_builder.shard.cml",
        "syslog/client.shard.cml",
    ],
    program: {
        data: "data/fuchsia-component-test-dart-tests",
    },
    use: [
        { protocol: "fuchsia.sys2.EventSource" },
        {
            event: [
                "started",
                "stopped",
            ],
            from: "framework",
        },
    ],
    offer: [
        {
            storage: "data",
            from: "parent",
            to: [ "#realm_builder" ],
        },
    ],
}
